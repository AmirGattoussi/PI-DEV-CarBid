                  </Label>
                  <Label text="Agency phone number:">
                     <font>
                        <Font name="System Italic" size="14.0" />
                     </font>
                  </Label>
                  <Label text="Car brand">
                     <font>
                        <Font name="System Italic" size="14.0" />
                     </font>
                  </Label>
                  <Label text="Car model:">
                     <font>
                        <Font name="System Italic" size="14.0" />
                     </font>
                  </Label>
                  <Label text="Reservation date:">
                     <font>
                        <Font name="System Italic" size="14.0" />
                     </font>
                  </Label>
                  <Label text="Address:">
                     <font>
                        <Font name="System Italic" size="14.0" />
                     </font>
                  </Label>
